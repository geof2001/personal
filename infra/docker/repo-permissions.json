{
  "Version": "2008-10-17",
  "Statement": [
    {
      "Sid": "cross-account",
      "Effect": "Allow",
      "Principal": {
        "AWS": [
          "arn:aws:iam::886239521314:root",
          "arn:aws:iam::181133766305:root"
        ]
      },
      "Action": [
        "ecr:GetDownloadUrlForLayer",
        "ecr:BatchGetImage",
        "ecr:BatchCheckLayerAvailability"
      ]
    },
    {
      "Sid": "jenkins",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::661796028321:root"
      },
      "Action": [
        "ecr:GetDownloadUrlForLayer",
        "ecr:BatchGetImage",
        "ecr:BatchCheckLayerAvailability",
        "ecr:PutImage",
        "ecr:InitiateLayerUpload",
        "ecr:UploadLayerPart",
        "ecr:CompleteLayerUpload"
      ]
    }
  ]
}